# Ignore some dirs
--ignore-dir=cache
--ignore-dir=logs
--ignore-dir=web/js
--ignore-dir=web/css
--ignore-dir=tags
--ignore-dir=_site
--ignore-dir=.subsplit

# Twig type
--type-set=database=.database
--type-set=scss=.scss
--type-set=sh=.sh
--type-set=twig=.twig
--type-set=yml=.yml
--type-set=json=.json
--type-set=feature=.feature
--type-set=erb=.erb
--type-set=md=.md
